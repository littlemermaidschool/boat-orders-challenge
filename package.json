{
	"name": "boat-orders",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"test": "jest -c jest.config.js",
		"tdd": "jest --watch -c jest.config.js",
		"lint": "eslint --ext .js --ext .ts src/ && prettier --check src/"
	},
	"engines": {
		"node": "12.21.0"
	},
	"dependencies": {
		"dayjs": "^1.10.4"
	},
	"prettier": "@pesky-fish/prettier",
	"lint-staged": {
		"src/**/*.{js,ts}": [
			"prettier --write src/",
			"eslint --ext .js --ext .ts src/ --fix"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"devDependencies": {
		"@pesky-fish/eslint-config": "1.0.0",
		"@pesky-fish/prettier": "1.0.0",
		"@types/jest": "26.0.21",
		"@typescript-eslint/eslint-plugin": "4.21.0",
		"@typescript-eslint/parser": "4.21.0",
		"babel-eslint": "10.1.0",
		"eslint": "7.19.0",
		"eslint-config-prettier": "8.1.0",
		"eslint-plugin-import": "2.22.1",
		"eslint-plugin-jest": "24.3.2",
		"husky": "4.3.8",
		"jest": "26.6.3",
		"lint-staged": "10.5.3",
		"prettier": "2.2.1",
		"ts-jest": "26.5.4",
		"typescript": "4.2.4"
	}
}
